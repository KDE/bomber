#, kde-format
msgctxt "NAME OF TRANSLATORS"
msgid "Your names"
msgstr "भवतः नामानि"

#, kde-format
msgctxt "EMAIL OF TRANSLATORS"
msgid "Your emails"
msgstr "भवतः ईमेलपत्राणि"

#: src/bomber.cpp:38 src/bomber.cpp:179
#, kde-format
msgctxt "Used to display the current level of play to the user"
msgid "Level: %1"
msgstr "स्तरः %1"

#: src/bomber.cpp:40 src/bomber.cpp:185
#, kde-format
msgctxt "Used to inform the user of their current score"
msgid "Score: %1"
msgstr "स्कोरः %1"

#: src/bomber.cpp:42 src/bomber.cpp:192
#, kde-format
msgctxt "Used to tell the user how many lives they have left"
msgid "Lives: %1"
msgstr "जीवति: %1"

#: src/bomber.cpp:83
#, kde-format
msgctxt "@option:check"
msgid "Play Sounds"
msgstr "ध्वनयः वादयन्तु"

#: src/bomber.cpp:88
#, kde-format
msgctxt "@action"
msgid "&Drop Bomb"
msgstr "&ड्रॉप बम्ब"

#: src/bomber.cpp:89
#, kde-format
msgctxt "@info:tooltip"
msgid "Drop bomb"
msgstr "बम्बं पातयन्तु"

#: src/bomber.cpp:90
#, kde-format
msgctxt "@info:whatsthis"
msgid "Makes the plane drop a bomb while flying."
msgstr "विमानं उड्डयनकाले बम्बं पातयति।"

#: src/bomber.cpp:134
#, kde-format
msgctxt ""
"Message displayed when player tries to quit a game that is currently running"
msgid "Do you really want to close the running game?"
msgstr "किं भवन्तः वास्तवमेव धावनक्रीडां पिधातुम् इच्छन्ति ?"

#: src/bomber.cpp:165
#, kde-format
msgid ""
"Congratulations!\n"
"You made it into the hall of fame."
msgstr ""
"अभिनन्दनम् !\n"
"त्वया तत् प्रसिद्धिभवनं कृतम्।"

#: src/bomber.cpp:169
#, kde-format
msgid "You gained a score of %1 point."
msgid_plural "You gained a score of %1 points."
msgstr[0] "भवता %1 अंकः प्राप्तः ।"
msgstr[1] "भवता %1 अंकस्य स्कोरः प्राप्तः ।"

#: src/bomber.cpp:170
#, kde-format
msgctxt "@title:window"
msgid "End of Game"
msgstr "क्रीडायाः समाप्तिः"

#: src/bomber.cpp:208
#, kde-format
msgctxt "Game over messaged displayed in the status bar"
msgid "Game over. Press '%1' for a new game"
msgstr "गेम ओवर। नूतनक्रीडायाः कृते '%1' नुदन्तु"

#. i18n: ectx: label, entry (PlaySounds), group (Sound)
#: src/bomber.kcfg:9
#, kde-format
msgid "Whether game sounds are played."
msgstr "क्रीडाध्वनयः क्रीड्यन्ते वा।"

#. i18n: ectx: ToolBar (mainToolBar)
#: src/bomberui.rc:14
#, kde-format
msgid "Main Toolbar"
msgstr "मुख्य साधनपट्टी"

#: src/bomberwidget.cpp:310
#, kde-format
msgctxt "Message show to the user when the game is loaded"
msgid ""
"Welcome to Bomber.\n"
"Click to start a game"
msgstr ""
"बम्बर् इत्यत्र स्वागतम्।\n"
"क्रीडां आरभ्यतुं क्लिक् कुर्वन्तु"

#: src/bomberwidget.cpp:313
#, kde-format
msgctxt "Message show to the user while the game is paused"
msgid "Paused"
msgstr "विरामः कृतः"

#: src/bomberwidget.cpp:316
#, kde-format
msgctxt "Message telling user which level they just completed"
msgid "You have successfully cleared level %1\n"
msgstr "भवता %1 स्तरः सफलतया स्वच्छः कृतः\n"

#: src/bomberwidget.cpp:317
#, kde-format
msgctxt "Message telling user which level they are about to start"
msgid "On to level %1."
msgstr "%1 स्तरं प्रति ।"

#: src/bomberwidget.cpp:320
#, kde-format
msgctxt "Used to tell the user that the game is over"
msgid "Game over."
msgstr "गेम ओवर।"

