#: src/bomber.cpp:165
#, kde-format
msgid ""
"Congratulations!\n"
"You made it into the hall of fame."
msgstr ""
"Gratulacje!\n"
"Trafiłeś na listę rekordów."

#: src/bomber.cpp:169
#, kde-format
msgid "You gained a score of %1 point."
msgid_plural "You gained a score of %1 points."
msgstr[0] "Zdobyłeś wynik równy %1 punktom."
msgstr[1] "Zdobyłeś wynik równy %1 punktom."
msgstr[2] "Zdobyłeś wynik równy %1 punktom."

#: src/bomber.cpp:170
#, kde-format
msgid "End of Game"
msgstr "Koniec gry"

#: src/bomber.cpp:208
#, kde-format
msgctxt "Game over messaged displayed in the status bar"
msgid "Game over. Press '%1' for a new game"
msgstr "Koniec gry. Naciśnij \"%1\", aby rozpocząć nową grę"

